#!/usr/bin/env zsh
######################################################
# SystemV-like script for initiating whatever you want
######################################################

# custom scripts at this directory at leveraged
# at runtime
# These scripts must deal with the following parameters: start
dot_init_custom_dir=${DOT_PATH}/lib/init.d

# start every single thing inside here
for script in $dot_init_custom_dir/*; do
    $script start
done
